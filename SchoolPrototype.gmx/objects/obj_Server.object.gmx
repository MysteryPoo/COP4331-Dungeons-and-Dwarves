        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Update simulation
for( var s = 0; s &lt; Players; ++s )
{
    var _lMap = PlayerList[| s ];
    var _dwarf = _lMap[? "Instance" ];
    if( _lMap[? "Map" ] == 5 )
    {
        var _pMap = _lMap[? "PositionMap" ];
        var _gMap = _lMap[? "GestureMap" ];
        if( !instance_exists( _dwarf ) )
        {
            var _random_spawn = irandom( 3 );
            _pMap[? "X" ] = SpawnX[ _random_spawn ];
            _pMap[? "Y" ] = SpawnY[ _random_spawn ];
            _dwarf = instance_create( _pMap[? "X" ], _pMap[? "Y" ], obj_Dwarf );
            _dwarf.netID = _lMap[? "Socket" ];
            _lMap[? "Instance" ] = _dwarf;
        }
        // Update Position of instance
        with( _dwarf )
        {
            if( State != "Death" )
            {
                var _dir = _pMap[? "Direction" ];
                var _spd = _pMap[? "Speed" ];
                var _x = _pMap[? "X" ] + _spd * dcos( _dir );
                var _y = _pMap[? "Y" ] - _spd * dsin( _dir );
                var _speed = point_distance( x, y, _x, _y ) / 3;
                direction = point_direction( x, y, _x, _y );
                if( point_distance( x, y, _x, _y ) &lt; 1 )
                {
                    MaxSpeed = 0;
                    x = _x;
                    y = _y;
                }
                else
                    MaxSpeed = _speed;
            }
        }
        // Update Gesture Objects
        with( obj_Bomb )
        {
            if( Owner == _dwarf )
            {
                hspeed = MaxSpeed * _gMap[? "X" ];
                vspeed = MaxSpeed * _gMap[? "Y" ];
            }
        }
    }
    else if( instance_exists( _dwarf ) )
        with( _dwarf )
            instance_destroy();
}
</string>
