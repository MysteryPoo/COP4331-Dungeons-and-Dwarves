var type_event = ds_map_find_value( async_load , "type" );
switch( type_event )
{
    case network_type_connect:
        var socket = ds_map_find_value( async_load , "socket" );
        if( ds_list_size( PlayerList ) &lt; 4 )
        {
            var _lMap = ds_map_create();
            _lMap[? "Socket" ]  = socket;
            _lMap[? "Instance" ] = noone;
            _lMap[? "Name" ]    = "Unnamed";
            _lMap[? "Map" ] = 0;
            _lMap[? "Ready" ] = false;
            var _pMap = ds_map_create();
            _pMap[? "X" ]       = 100;
            _pMap[? "Y" ]       = 100;
            _pMap[? "Direction" ] = 0;
            _pMap[? "Speed" ] = 0;
            _lMap[? "PositionMap" ] = _pMap;
            var _gMap = ds_map_create();
            _gMap[? "X" ] = 0;
            _gMap[? "Y" ] = 0;
            _lMap[? "GestureMap" ] = _gMap;
            ds_list_add( PlayerList, _lMap );
            StartTimer = 30 * 10;
        }
        if( ds_list_size( PlayerList ) &gt; 1 )
            State = "Gathering";
        ds_list_add( SocketList, socket );
        // Update player count on Lobby Server
        buffer_seek( Buffer, buffer_seek_start, 0 );
        buffer_write( Buffer, buffer_u8, 2 );
        buffer_write( Buffer, buffer_u8, ds_list_size( PlayerList ) );
        network_send_packet( LobbyServer, Buffer, buffer_tell( Buffer ) );
        break;
    case network_type_disconnect:
        var socket = ds_map_find_value( async_load , "socket" );
        for( var s = 0; s &lt; ds_list_size( SocketList ); ++s )
        {
            if( SocketList[| s ] == socket )
                ds_list_delete( SocketList, s );
        }
        for( var s = 0; s &lt; ds_list_size( PlayerList ); ++s )
        {
            var _lMap = PlayerList[| s ];
            if( _lMap[? "Socket" ] == socket )
            {
                var _inst = _lMap[? "Instance" ];
                if( instance_exists( _inst ) )
                    with( _inst )
                        instance_destroy();
                ds_map_destroy( _lMap[? "PositionMap" ] );
                ds_map_destroy( _lMap[? "GestureMap" ] );
                ds_map_destroy( _lMap );
                ds_list_delete( PlayerList, s );
                // Notify clients of the disconnect
                buffer_seek( Buffer, buffer_seek_start, 0 );
                buffer_write( Buffer, buffer_u8, socket );
                for( var s = 0; s &lt; ds_list_size( SocketList ); ++s )
                {
                    network_send_packet( SocketList[| s ], Buffer, buffer_tell( Buffer ) );
                }
            }
        }
        // Update player count on Lobby Server
        buffer_seek( Buffer, buffer_seek_start, 0 );
        buffer_write( Buffer, buffer_u8, 2 );
        buffer_write( Buffer, buffer_u8, ds_list_size( PlayerList ) );
        network_send_packet( LobbyServer, Buffer, buffer_tell( Buffer ) );
        break;
    case network_type_data:
        var buffer = ds_map_find_value( async_load , "buffer" );
        var socket = ds_map_find_value( async_load , "id" );
        buffer_seek( buffer , buffer_seek_start , 0 );
        scr_SReceivedPacket( buffer , socket );
        break;
}
