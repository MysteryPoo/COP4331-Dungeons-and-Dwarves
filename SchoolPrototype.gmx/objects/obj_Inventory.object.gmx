            <string>/// Open
if( State == "Open_Start" )
{
    State = "Open";
    SlideTimer = 0;
    audio_play_sound( sound_inventory, 2, false );
}
if( State == "Open" )
{
    x = -32 * WeaponCount + 32 * WeaponCount * dsin( SlideTimer );
    if( SlideTimer &lt; 90 )
        SlideTimer += 90 / ( WeaponCount + 5 );
    else
        State = "Opened_Start";
}
