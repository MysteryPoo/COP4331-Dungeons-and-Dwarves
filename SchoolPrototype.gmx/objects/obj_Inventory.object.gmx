        switch( Inventory[| w ] )
        {
            case obj_Bow:
            case obj_Bow_2:
            case obj_Bow_3:
                if( Ammo[? "Arrows" ] &gt; 0 )
                    draw_sprite_ext( SpriteMap[? "Bow" ], 0, _x, _y, 0.2, 0.2, 0, c_white, 1 );
                else
                    draw_sprite_ext( SpriteMap[? "Bow" ], 0, _x, _y, 0.2, 0.2, 0, c_red, 1 );
                draw_set_halign( fa_center );
                draw_text( _x, _y, string( Ammo[? "Arrows" ] ) );
                draw_set_halign( fa_left );
                break;
            case obj_Spike:
            case obj_Spike_2:
            case obj_Spike_3:
                draw_sprite_ext( SpriteMap[? "Spike" ], 0, _x, _y, 0.1, 0.1, 0, c_white, 1 );
