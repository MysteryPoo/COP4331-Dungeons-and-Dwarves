            <kind>1</kind>
            <string>/// Multi Touch Device 0
Touched = false;
if( device_mouse_check_button_pressed( 0, mb_left ) )
{
    /// Enable gesture tracking
    mxs = device_mouse_x( 0 ) - view_xview[ 0 ];
    mys = device_mouse_y( 0 ) - view_yview[ 0 ];
    dist = 0;
    max_dist = 0;
    scale = 0;
}

if( device_mouse_check_button( 0, mb_left ) )
{
    Touched = true;
    /// Update Touch
    mx = device_mouse_x( 0 ) - view_xview[ 0 ];
    my = device_mouse_y( 0 ) - view_yview[ 0 ];
    dist = point_distance( mxs, mys, mx, my );
    if( dist &gt; max_dist )
        max_dist = dist;
    if( max_dist &gt; 0 )
        scale = dist / max_dist;
    direction = point_direction( mxs, mys, mx, my );
    visible = true;
}

if( device_mouse_check_button_released( 0, mb_left ) )
{
    /// Disable gesture tracking
    scale = 0;
    visible = false;
}
</string>
