        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Hit
if( State == "Hit_Start" )
{
    State = "Hit";
    StateTimer = 15;
    // Change animation

    /// Create the emitter
    var emit;
    emit = instance_create(x, y, obj_Emitter);
    emit.sid = sound_grunt_loud;
    emit.fref = 10;
    emit.fmax = 20;
}
if( State == "Hit" )
{
    var _newCol;
    if( image_blend == c_white )
        _newCol = c_red;
    else
        _newCol = c_white;
    if( StateTimer mod 5 == 0 )
        image_blend = _newCol;
    if( StateTimer mod 2.5 == 0 &amp;&amp; StateTimer != 15 )
        speed *= 0.1;
    if( speed &lt; 1 )
        speed = 0;
    if( StateTimer == 0 )
    {
        State = "Idle_Start";
        image_blend = c_white;
    }
    if( HitPoints &lt;= 0 )
    {
        State = "Death_Start";
        image_blend = c_white;
    }
}
