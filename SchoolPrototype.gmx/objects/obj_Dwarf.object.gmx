        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Death
if( State == "Death_Start" )
{
    /// Create the emitter
    var emit;
    emit = instance_create(x, y, obj_Emitter);
    emit.sid = sound_scream;
    emit.fref = 10;
    emit.fmax = 20;
    
    visible = false;
    var _Tombstone = instance_create( x, y, obj_TombStone );
    if( instance_exists( obj_Client ) )
    {
        for( var s = 0; s &lt; ds_list_size( obj_Client.SocketList ); ++s )
        {
            var _lMap = obj_Client.SocketList[|s];
            if( _lMap[?"Instance"] == id )
                _Tombstone.Name = _lMap[?"Name"];
        }
    }
    image_index = 0;
    image_speed = 1;
    speed = 0;
    State = "Death";
    StateTimer = 30 * 2;
}
if( State == "Death" )
{
    if( image_index &gt;= image_number - 1 )
        image_speed = 0;
    if( StateTimer == 0 )
    {
        if( room == rm_Client &amp;&amp; global.MyInst == id )
            room_goto( rm_MainMenu );
        else if( room == rm_Server )
        {
            for( var p = 0; p &lt; ds_list_size( obj_Server.PlayerList ); ++p )
            {
                var _lMap = obj_Server.PlayerList[|p];
                if( _lMap[?"Instance"] == id )
                {
                    var _deadsocket = _lMap[?"Socket"];
                    with( obj_Server )
                    {
                        network_destroy( _lMap[?"Socket"] );
                        // Really messy code below
                        var socket = _deadsocket;
                        for( var s = 0; s &lt; ds_list_size( SocketList ); ++s )
                        {
                            if( SocketList[| s ] == socket )
                                ds_list_delete( SocketList, s );
                        }
                        for( var s = 0; s &lt; ds_list_size( PlayerList ); ++s )
                        {
                            var _lMap = PlayerList[| s ];
                            if( _lMap[? "Socket" ] == socket )
                            {
                                var _inst = _lMap[? "Instance" ];
                                if( instance_exists( _inst ) )
                                    with( _inst )
                                        instance_destroy();
                                ds_map_destroy( _lMap[? "PositionMap" ] );
                                ds_map_destroy( _lMap[? "GestureMap" ] );
                                ds_map_destroy( _lMap );
                                ds_list_delete( PlayerList, s );
                                // Notify clients of the disconnect
                                buffer_seek( Buffer, buffer_seek_start, 0 );
                                buffer_write( Buffer, buffer_u8, 4 );
                                buffer_write( Buffer, buffer_u8, socket );
                                for( var s = 0; s &lt; ds_list_size( SocketList ); ++s )
                                {
                                    network_send_packet( SocketList[| s ], Buffer, buffer_tell( Buffer ) );
                                }
                            }
                        }
                        // Update player count on Lobby Server
                        buffer_seek( Buffer, buffer_seek_start, 0 );
                        buffer_write( Buffer, buffer_u8, 2 );
                        buffer_write( Buffer, buffer_u8, ds_list_size( PlayerList ) );
                        network_send_packet( LobbyServer, Buffer, buffer_tell( Buffer ) );
                        // End of really messy code
                    }
                }
            }
            instance_destroy();
        }
